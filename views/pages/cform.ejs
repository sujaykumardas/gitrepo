<html>
<head>
<script type="text/javascript">
    function validate() 
    {
       var str= /^[A-Za-z]+$/;
       var num= /^[0-9]{2}$/;
       var yn= /^(y|n){1}$/;
       var dof= /^(0?[1-9]|[12][0-9]|3[01])[\/\-](0?[1-9]|1[012])[\/\-]\d{4}$/;
       var emid= /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
       var ph =/^\d{10}$/;
       var cgpaf= /^[0-9]{1}\.{1}[0-9]+$/;
       var pswd= /^(?=.*[A-Za-z])(?=.*\d)(?=.*[$@$!%*#?&])[A-Za-z\d$@$!%*#?&]{8,}$/;
       var cpat= /^sU8Nki$/i;
       var pin= /^[0-9]{6}$/;
       //document.body.innerhtml.text.style.color="red";
       //document.body.innerhtml.style.borderColor="red";
       //document.body.innerhtml.text.style.visibility="hidden";


       var ch=0;
        if(!str.test(myform.fname.value))
        {
           ch=1;
           //document.getElemenyById("fname").style.borderColor= "red";
           document.getElementById('errfn').innerHTML="<br>this is invalid first name<br></span>";
           document.getElementById('fname').innerhtml.style.visibility="visible";
           
           
     
        }
        if(!cpat.test(myform.cpt.value))
        {
           ch=1;
           //document.getElemenyById("fname").style.borderColor= "red";
           document.getElementById('errcpt').innerHTML="<br>this is invalid captcha<br></span>";
           document.getElementById('cpt').innerhtml.style.visibility="visible";
           
        }
        if(!pin.test(myform.pnc.value))
        {
           ch=1;
           //document.getElemenyById("fname").style.borderColor= "red";
           document.getElementById('errpnc').innerHTML="<br>this is invalid pincode<br></span>";
           document.getElementById('pnc').innerhtml.style.visibility="visible";
           
        }
        if(!str.test(myform.lname.value))
        {
           ch=1;
           document.getElementById('errln').innerHTML="<br>this is invalid last name<br></span>";
           document.getElementById('lname').innerhtml.style.visibility="visible";
           
        }
        if(!num.test(myform.age.value))
        {
           ch=1;
           document.getElementById('errno').innerHTML="<br>this is invalid age<br></span>";
           document.getElementById('age').innerhtml.style.visibility="visible";
        }
        if(!yn.test(myform.ms.value))
        {
           ch=1;
           document.getElementById('errms').innerHTML="<br>this is invalid married status<br></span>";
           document.getElementById('ms').innerhtml.style.visibility="visible";
           
        }
        if(!ph.test(myform.phn.value))
        {
           ch=1;
           document.getElementById('errph').innerHTML="<br>this is invalid mobile number<br></span>";
           document.getElementById('phn').innerhtml.style.visibility="visible";
        }
        if(!dof.test(myform.dob.value))
        {
           ch=1;
           document.getElementById('errdb').innerHTML="<br>this is invalid birth date<br></span>";
           document.getElementById('dob').innerhtml.style.visibility="visible";
        }
        if(!emid.test(myform.mid.value))
        {
           ch=1;
           document.getElementById('erreid').innerHTML="<br>this is invalid mail address<br></span>";
           document.getElementById('mid').innerhtml.style.visibility="visible";
        }
        if(!cgpaf.test(myform.gpa.value))
        {
           ch=1;
           document.getElementById('errcg').innerHTML="<br>this is invalid cgpa<br></span>";
           document.getElementById('gpa').innerhtml.style.visibility="visible";
           
        }
        if(!pswd.test(myform.pid.value))
        {
           ch=1;
           document.getElementById('errpid').innerHTML="<br>password min 8 char atleast 1 letter one num and one special character<br></span>";
           document.getElementById('pid').innerhtml.style.visibility="visible";
        }
        if(ch==0)
        {
           
           document.getElementById('button').innerHTML="<br>Form submitted successfully<br></span>";
           document.getElementById('but').innerhtml.style.visibility="visible";
           
        }
        
        
  
    }
</script>
</head>
<body>
    <form name="myform">
          <div style="color: black" style="visibility: hidden">
          
          First_Name
         <input type=text id="fname" name="fname" style="color: red" style="visibility: hidden" />
         
         <br> <br>
          Last_Name
         <input type=text id="lname" name="lname" style="color: red" style="visibility: hidden"/>
         
         <br> <br>
          Age
         <input type=number id="age" name=age style="color: red" style="visibility: hidden" />
         
         <br> <br>
          DOB
         <input type=date id="dob" name=dob  style="color: red" style="visibility: hidden"/>
         
         <br> <br>
          MailId
         <input type=email id="mid" name=mid style="color: red" style="visibility: hidden" />
         
         <br> <br>
          Married(Y/N)
         <input type=text id="ms" name=ms style="color: red" style="visibility: hidden"/>
         
         <br> <br>
         Mobile no (Without code)
         <input type=number id="phn" name=phn style="color: red" style="visibility: hidden" />
         
         <br> <br>
         AggregateCGPA(x.y)
         <input type=text id="gpa" name=gpa style="color: red" style="visibility: hidden" />
         
         <br> <br>
         Pincode
         <input type=number id="pnc" name="pnc"  style="color: red" style="visibility: hidden"/>
         
         <br> <br>
         Password
         <input type=password id="pid" name=pid  style="color: red" style="visibility: hidden"/>
         
         <br> <br>
         Enter the text as it is<br> 
         sU8Nki
         <input type=text id="cpt" name=cpt  style="color: red" style="visibility: hidden"/>
         
         <br> <br>
         <input  id="but" type=button onclick="validate()" value=SUBMIT />
          
        </div>
    </form>
</body>
